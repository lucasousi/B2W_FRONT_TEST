{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "pokestore-b2w",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "aquamons-store": {
      "root": "apps/aquamons-store",
      "sourceRoot": "apps/aquamons-store/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["{options.outputPath}"],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/aquamons-store",
            "outputPath": "dist/apps/aquamons-store"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/aquamons-store"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
            "cwd": "apps/aquamons-store"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/aquamons-store"
          }
        }
      }
    }
  }
}
